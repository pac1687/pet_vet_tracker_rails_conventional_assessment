  <%= render("layouts/errors", :object => @veterinarian) %>

  <%= form_for(@veterinarian) do |f| %>
    <div class="form-group">
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.label :phone %><br />
      <%= f.text_field :phone %>
    </div>
    <div class="form-group">
      <%= f.label :email %><br />
      <%= f.text_field :email %>
    </div>
    <div class="form-group">
      <%= f.label :address %><br />
      <%= f.text_field :address %>
    </div>
    <%= f.submit(:class => "btn btn-primary") %>
  <% end %>