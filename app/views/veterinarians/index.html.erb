<div class="page-header">
  <h1><%= current_user.name %>'s Veterinarians</h1>
</div>
<div class="container">
  <ul>
    <% @veterinarians.each do |veterinarian| %>
      <li><h3><%= link_to "#{veterinarian.name}", veterinarian_path(veterinarian) %></h3></li>
    <% end %>
  </ul>
</div>

<div class="page-header">
  <h2>New Veterinarian</h2>
</div>

<div class="container">
  <%= render('form') %>
</div>

